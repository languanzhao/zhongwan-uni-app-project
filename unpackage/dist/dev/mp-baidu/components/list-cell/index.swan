<view class="list-cell"><block s-for="{{list}}" s-for-item="item" s-for-index="index" s-key="index"><view data-event-opts="{{[['tap',[['link',['$0'],[[['list','',index,'path']]]]]]]}}" class="item" bindtap="__e"><view class="item-left"><block s-if="{{item.icon}}"><text class="{{['iconfont',item.icon]}}" style="{{'color:'+(item.color)+';'}}"></text></block><text class="title">{{item.title}}</text></view><view class="item-right"><block s-if="{{item.tip}}"><text class="tip">{{item.tip}}</text></block><text class="iconfont icon-you"></text></view></view></block></view>